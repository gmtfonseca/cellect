{
  "name": "Cellect",
  "description": "Select and aggregate cells",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "cellect.css"
      ],
      "js": [
        "cellect.js"
      ],
      "match_about_blank": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "total.html"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": []
    }
  ]
}